{
  "manifest_version": 3,
  "name": "form auto input",
  "description": "form auto input",
  "version": "0.1.0",
  "icons": {
    "16": "images/icon16.jpeg",
    "32": "images/icon32.jpeg",
    "48": "images/icon48.jpeg",
    "128": "images/icon128.jpeg"
  },
  "content_scripts": [
    {
      "matches": ["*"],
      "js": ["src/content.ts"],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_title": "form auto input"
  }
}
